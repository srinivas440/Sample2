[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/srinivas440
[access "refs/for/refs/*"]
	read = group user/srinivas440
	push = group user/srinivas440
	label-Code-Review = -2..+2 group user/srinivas440
	label-Verified = -1..+1 group user/srinivas440
	rebase = group user/srinivas440
	removeReviewer = group user/srinivas440
	submit = group user/srinivas440
[access "refs/heads/*"]
	read = group user/srinivas440
	create = group user/srinivas440
	push = +force group user/srinivas440
	pushMerge = group user/srinivas440
[access "refs/tags/*"]
	push = group user/srinivas440
	push = +force group user/srinivas440
